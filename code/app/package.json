{
  "name": "rcn.io",
  "version": "0.0.1",
  "description": "Source code for rcn.io, project that makes cycling in USA awesome.",
  "scripts": {
    "clean": "rimraf dist",
    "clean:all": "npm run clean && rimraf bower_components && rimraf node_modules",
    "build:prod": "NODE_ENV=production webpack -p --config webpack.config.prod.js --progress",
    "build": "npm run clean && npm run build:prod",
    "start": "node devServer.js",
    "eslint": "eslint src/** --ext .js,.jsx || true",
    "eslint:fix": "eslint src/** --ext .js,.jsx --fix || true",
    "reinstall": "npm run clean:all && bower i && npm i"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Restuta/rcn.io.git"
  },
  "private": true,
  "keywords": [
    "rcn",
    "bike",
    "cycling",
    "racing"
  ],
  "author": "Anton Vynogradenko <restuta8@gmail.com> (http://restuta.me)",
  "license": "",
  "bugs": {
    "url": "https://github.com/Restuta/rcn.io/issues"
  },
  "homepage": "http://rcn.io",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.7.2",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "css-loader": "0.14.5",
    "eslint": "~2.2.0",
    "eslint-plugin-react": "^4.1.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "imports-loader": "^0.6.5",
    "node-sass": "^3.3.3",
    "postcss-loader": "^0.8.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.2",
    "redbox-react": "^1.2.2",
    "rimraf": "^2.4.4",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "svg-inline-loader": "^0.4.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "classnames": "^2.2.1",
    "moment": "^2.11.2",
    "react": "^15.0.0-rc.1",
    "react-dom": "^15.0.0-rc.1",
    "react-pure-render": "^1.0.1",
    "react-router": "^2.0.1",
    "svg-inline-react": "^1.0.0"
  }
}
